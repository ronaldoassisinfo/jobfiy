        <% vagas.forEach( vaga => { %>
            <li>
                <%- vaga.titulo  %> |
                <a href="/admin/vagas/delete/<%- vaga.id %>">Excluir</a>
            </li>
        <% }) %>
    </ul>
</div>
<%- include('footer') %>